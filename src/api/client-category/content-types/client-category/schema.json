{
  "kind": "collectionType",
  "collectionName": "client_categories",
  "info": {
    "singularName": "client-category",
    "pluralName": "client-categories",
    "displayName": "Client Category",
    "description": "Groups of clients displayed on the clients page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "order": {
      "type": "integer"
    },
    "clients": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::client.client",
      "mappedBy": "category"
    }
  }
}
