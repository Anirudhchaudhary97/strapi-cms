{
  "kind": "collectionType",
  "collectionName": "faqs",
  "info": {
    "singularName": "faq",
    "pluralName": "faqs",
    "displayName": "faq"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "question": {
      "type": "string",
      "required": true
    },
    "answer": {
      "type": "richtext",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": ["General", "Services", "Billing", "Operations", "Other"],
      "default": "General"
    },
    "order": {
      "type": "integer"
    }
  }
}
