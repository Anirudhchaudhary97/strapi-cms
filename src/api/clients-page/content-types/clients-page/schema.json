{
  "kind": "singleType",
  "collectionName": "clients_pages",
  "info": {
    "singularName": "clients-page",
    "pluralName": "clients-pages",
    "displayName": "Clients Page",
    "description": "Clients page sections and category listings"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "richtext"
    },
    "stats": {
      "type": "component",
      "repeatable": true,
      "component": "shared.stat-item"
    },
    "sectionTitle": {
      "type": "string"
    },
    "sectionSubtitle": {
      "type": "text"
    },
    "clientCategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::client-category.client-category"
    },
    "cta": {
      "type": "component",
      "repeatable": false,
      "component": "common.cta-card"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
