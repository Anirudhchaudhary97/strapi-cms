{
  "kind": "singleType",
  "collectionName": "contact_pages",
  "info": {
    "singularName": "contact-page",
    "pluralName": "contact-pages",
    "displayName": "Contact Page",
    "description": "Contact page form messaging, location, and map"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "richtext"
    },
    "heroEyebrow": {
      "type": "string"
    },
    "heroImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    },
    "successMessage": {
      "type": "string"
    },
    "errorMessage": {
      "type": "string"
    },
    "locationTitle": {
      "type": "string"
    },
    "locationText": {
      "type": "text"
    },
    "mapDescription": {
      "type": "text"
    },
    "phonesTitle": {
      "type": "string"
    },
    "emailsTitle": {
      "type": "string"
    },
    "businessHoursTitle": {
      "type": "string"
    },
    "businessHoursBody": {
      "type": "richtext"
    },
    "mapTitle": {
      "type": "string"
    },
    "mapEmbedUrl": {
      "type": "string"
    },
    "contactIntroTitle": {
      "type": "string"
    },
    "contactIntroSubtitle": {
      "type": "text"
    },
    "contactChannels": {
      "type": "component",
      "repeatable": true,
      "component": "common.contact-channel"
    },
    "formTitle": {
      "type": "string"
    },
    "formSubtitle": {
      "type": "text"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
